    <script src="vendor/bootstrap/js/jquery.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.js"></script>
    <script>
      /**
       * Task: read project divs, remove them, and then insert the divs back in
       *       randomly.
       */

      $(document).ready(function() {
        var rows = $('#project_list .row');
        var projects = $('#project_list .row .col').toArray();

        $('#project_list .col').remove();

        var i = 0;
        while (projects.length) {
          var numRow = Math.floor(i++/3);
          var randomProjIndex = Math.floor(Math.random()*projects.length);
          $(rows[numRow]).append($(projects[randomProjIndex]));
          projects.splice(randomProjIndex, 1);
        }

        $('#project_list').prevAll('div.page-header:first').children('h2').append(" <small>in random order!</small>");
      });
    </script>
